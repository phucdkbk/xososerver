<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <display-name>xososerver</display-name>
    <listener>
        <description>ServletContextListener</description>
        <listener-class>com.alandk.lottery.util.SchedulePlugin</listener-class>
    </listener>
    <servlet>
        <servlet-name>LotteryResult</servlet-name>
        <servlet-class>com.alandk.lottery.servlet.LotteryResult</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ShowResult</servlet-name>
        <servlet-class>com.alandk.lottery.servlet.ShowResult</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateResult</servlet-name>
        <servlet-class>com.alandk.lottery.servlet.UpdateResult</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ThongkeLogan</servlet-name>
        <servlet-class>com.alandk.lottery.servlet.ThongkeLogan</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ThongkeItNhieu</servlet-name>
        <servlet-class>com.alandk.lottery.servlet.ThongkeItNhieu</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LotteryResult</servlet-name>
        <url-pattern>/LotteryResult</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ShowResult</servlet-name>
        <url-pattern>/ShowResult</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateResult</servlet-name>
        <url-pattern>/UpdateResult</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ThongkeLogan</servlet-name>
        <url-pattern>/ThongkeLogan</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ThongkeItNhieu</servlet-name>
        <url-pattern>/ThongkeItNhieu</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
